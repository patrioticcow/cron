{
  "manifest_version": 2,
  "name": "Cron Job Generator",
  "author": "Cristi C cristi.catea@gmail.com",
  "description": "Cron job generator",
  "version": "1.4.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "js/jquery-1.9.1.min.js",
        "js/injected.js"
      ],
      "all_frames": false
    }
  ]
}
